generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Gnome {
  id   String @id @default(uuid())
  name String

  latitude  Float
  longitude Float
}

model User {
  id         String @id @default(uuid())
  googleId   String
  name       String
  email      String
  pictureUrl String
}
