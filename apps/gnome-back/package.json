{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn generate && nest build",
    "lint": "npx @biomejs/biome check --write ./src && prisma format",
    "lint:ci": "npx @biomejs/biome ci ./src --diagnostic-level=error",
    "studio": "prisma studio --port 5556",
    "generate": "prisma generate && prisma format",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:migrate:prod": "prisma migrate deploy && yarn start:prod",
    "start:prod": "node dist/main",
    "seed": "prisma db seed"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.4.1",
    "@repo/shared": "*",
    "@turf/turf": "^7.3.0",
    "@types/minio": "^7.1.1",
    "@types/multer": "^1.4.12",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "google-auth-library": "^9.15.0",
    "minio": "^8.0.5",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "latest",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.17.27",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.5.0",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.8.2"
  },
  "name": "gnome-back",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
