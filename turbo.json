{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:ci": {
      "dependsOn": ["^lint:ci"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
